array1 = [689354, 976840, 307322, 312916, 449961, 431224, 382841, 719725, 468820, 196665, 612501, 818754, 805756]
array2 = [854949, 673789, 211007, 819723, 297359, 310329, 771289, 103250, 227222, 262860, 342707, 272021, 968628]

rezult = []

for i in range(len(array1)):
    rezult.append(array1[i] + array2[i])
  
    
print(rezult)
